{
    "_version": "",
    "sap.cloud": {
        "service": "nbspocovpuiapprouter"
    },
    "sap.app": {
        "id": "nbs.poc.ovp.ui.app.report.ovporder",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": []
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZPOCO_UI_ORDER_CHART_O2/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "ZPOCO_UI_ORDER_CHART_O2_VAN",
                        "annotation"
                    ],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "ZPOCO_UI_ORDER_CHART_O2_VAN": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZPOCO_UI_ORDER_CHART_O2_VAN',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/ZPOCO_UI_ORDER_CHART_O2_VAN.xml"
                }
            },
            "annotation": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotation.xml",
                "settings": {
                    "localUri": "annotations/annotation.xml"
                }
            }
        },
        "offline": false,
        "sourceTemplate": {
            "id": "OVP.smartovptemplate",
            "version": "1.41.1"
        },
        "crossNavigation": {
            "inbounds": {
                "nbs-poc-ovp-ui-app-report-ovporder-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "NBS_POC_OVP_ORDER",
                    "action": "report",
                    "title": "{{AppTitle}}",
                    "subTitle": "",
                    "icon": "sap-icon://Chart-Tree-Map"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.65.0",
            "libs": {
                "sap.ovp": {}
            },
            "components": {}
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties",
                "preload": false
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "mainService": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultCountMode": "Inline"
                }
            }
        },
        "extends": {
            "extensions": {}
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "analytical"
    },
    "sap.ovp": {
        "globalFilterModel": "mainService",
        "globalFilterEntityType": "zpoco_c_order_chartType",
        "containerLayout": "resizable",
        "enableLiveFilter": true,
        "considerAnalyticalParameters": false,
       "cards": {
            "orders_card": {
                "model": "mainService",
                "template": "sap.ovp.cards.charts.analytical",
                "settings": {
                    "title": "Status L1",
                    "subTitle": " ",
                    "entitySet": "CardStatusL1",
                    "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
                    "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#Default",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#STSL1"
                }
            },
            "orders_card_2": {
                "model": "mainService",
                "template": "sap.ovp.cards.charts.analytical",
                "settings": {
                    "title": "Status L2",
                    "subTitle": " ",
                    "entitySet": "CardStatusL2",
                    "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
                    "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#Default",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#STSL2"
                }
            },
            "orders_card_3": {
                "model": "mainService",
                "template": "sap.ovp.cards.charts.analytical",
                "settings": {
                    "title": "Therapy",
                    "subTitle": " ",
                    "entitySet": "CardTherapy",
                    "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
                    "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#Default",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#Therapy"
                }
            },
            "order_card_4": {
                "model": "mainService",
                "template": "sap.ovp.cards.table",
                "settings": {
                    "title": "Therapy By Plant",
                    "entitySet": "CardTherapyPlantTable",
                    "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
                    "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#Default",
                    "selectionPresentationAnnotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant"
                }
            },
            "orders_card_5": {
                "model": "mainService",
                "template": "sap.ovp.cards.charts.analytical",
                "settings": {
                    "title": "Maximum Quantity of Therapy",
                    "subTitle": "",
                    "entitySet": "CardTherapyPlantMax",
                    "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
                    "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#Default",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart",
                    "dataPointAnnotationPath": "com.sap.vocabularies.UI.v1.DataPoint#MaxQuantity"
                }
            },
            "orders_card_6": {
                "model": "mainService",
                "template": "sap.ovp.cards.list",
                "settings": {
                    "title": "Therapies By Date",
                    "entitySet": "CardTherapyDateMax",
                    "listFlavor": "bar",
                    "listType": "extended",
                    "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
                    "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#Default",
                    "annotationPath": "com.sap.vocabularies.UI.v1.LineItem",
                    "defaultSpan":"auto"
                }
            },
            "orders_card_7": {
                "model": "mainService",
                "template": "sap.ovp.cards.linklist",
                "settings": {
                    "title": "Plant List",
                    "entitySet": "CardPlantList",
                    "listType": "extended",
                    "listFlavor": "standard",
                    "headerAnnotationPath": "com.sap.vocabularies.UI.v1.HeaderInfo"
                }
            },
           "slots_plants_aggregated": {
                "model": "mainService",
                "template": "sap.ovp.cards.charts.analytical",
                "settings": {
                    "title": "Consumed Slots",
                    "subTitle": "Aggregation by plants",
                    "entitySet": "Slots_plants",
                    "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
                    "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#Default",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#Slots"
                }
            },
            "slots_dates_aggregated": {
                "model": "mainService",
                "template": "sap.ovp.cards.charts.analytical",
                "settings": {
                    "title": "Consumed Slots",
                    "subTitle": "Aggregation by dates",
                    "entitySet": "Slots_dates",
                    "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
                    "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#Default",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#Slots"
                }
            }, 
                            
            "plants_aggregated": {
                "model": "mainService",
                "template": "sap.ovp.cards.charts.analytical",
                "settings": {
                    "title": "Plants",
                    "subTitle": "Aggregation by plants",
                    "listType": "extended",
                    "entitySet": "Plants",
                    "identificationAnnotationPath": "com.sap.vocabularies.UI.v1.Identification",
                    "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#Default",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#Plants"
                    }
                }
            }
        }
}